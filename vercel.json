{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js",
      "headers": {
        "Access-Control-Allow-Methods": "GET,POST,OPTIONS",
        "Access-Control-Allow-Origin": "https://morning-news-frontend-ebon.vercel.app/",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
